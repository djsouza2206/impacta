CREATE TABLE REL_CONTROLE_FINANCEIRO_BASICO(
	REGRA   	VARCHAR(50)     DEFAULT ' ',
	CATEG		VARCHAR(50)	DEFAULT ' ',
	SUBCATEG	VARCHAR(50)	DEFAULT ' ',

	MES		VARCHAR(10)	DEFAULT ' ',
	VALOR		DECIMAL(14,2)	DEFAULT   0,
	PERC		DECIMAL(14,2)	DEFAULT   0,
	CREDITO		DECIMAL(14,2)	DEFAULT   0,
	ANOMES		VARCHAR(20)	DEFAULT ' '
)TYPE=MYISAM;


